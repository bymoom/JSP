<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>

<%
	String source = "010-1234-5678";
	StringTokenizer stn = new StringTokenizer(source, "-"); //"-" : 뭘로 자를건지 구분해주는 구분자
	
	while(stn.nextToken()){ //잘라달라고 할때마다 잘라줌
		
	}
%>

