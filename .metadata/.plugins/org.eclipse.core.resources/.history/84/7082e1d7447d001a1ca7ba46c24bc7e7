<%
	int score=85;

	String scroeLevel = "F";
	
	if(score > 90){
		scroeLevel = "A";
	}else if(score > 80){
		scroeLevel = "B";
	}else if(score > 70){
		scroeLevel = "C";
	}else{
		scroeLevel = "F";
	}
	
	out.println(scroeLevel);
%>

