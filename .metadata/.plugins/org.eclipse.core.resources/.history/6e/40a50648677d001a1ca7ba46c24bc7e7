<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ page trimDirectiveWhitespaces="true" %>

<script>
	if(window.opener){
		alert("세션이 만료되었습니다.\n로그인 화면으로 이동합니다.");
		window.close();
		window.opener.location.href = "commons/loginForm";
	}else{
		alert("세션이 만료되었습니다.\n로그인 화면으로 이동합니다.");
		window.location.reload(true);
	}
</script>